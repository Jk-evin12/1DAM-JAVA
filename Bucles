
package Bucles; /*Bucle FOR*/

import java.util.Scanner;

public class BuclesFor {

	public static void main(String[] args) {
		
	/*ENUNCIADO: Realiza un programa que dado por teclado  muestre 
en pantalla todos los números hasta ese límite (empezando por 1)*/	
	 
	int numero;
		Scanner teclado= new Scanner(System.in);
		
		System.out.println("Ingresa tu número: ");
		numero=teclado.nextInt();
		
		for(int i=0; i<numero; i++) {
			System.out.println(i+1);
		}
	
	}		
}
